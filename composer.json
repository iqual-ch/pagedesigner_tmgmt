{
  "name": "iqual/pagedesigner_tmgmt",
  "description": "Integrate pagedesigner with deepl",
  "type": "drupal-custom-module",
  "keywords": [
    "Drupal",
    "Editing"
  ],
  "homepage": "https://github.com/iqual-ch/pagedesigner_tmgmt",
  "support": {
    "issues": "https://github.com/iqual-ch/pagedesigner_tmgmt/issues",
    "source": "https://github.com/iqual-ch/pagedesigner_tmgmt"
  },
  "license": "GPL-2.0+",
  "require": {
    "drupal/pagedesigner": "^2.0",
    "drupal/tmgmt": "^1.0",
    "drupal/tmgmt_deepl": "^2.1",
    "drupal/hook_post_action": "^1.2"
  },
  "extra": {
    "patches": {
      "drupal/pagedesigner": {
        "#3348718: Use entity Id as key in content array of pagedesigner element": "https://www.drupal.org/files/issues/2023-03-17/3348718_refactor_content_array_for_tmgmt.patch"
      },
      "drupal/tmgmt_deepl": {
        "Make tmgmt_deepl compatible with PHP7.3": "https://raw.githubusercontent.com/iqual-ch/pagedesigner_tmgmt/issue/US-349_cleanup/patches/tmgmt_deepl_php7-3_compatibility.diff"
      }
    }
  }
}
